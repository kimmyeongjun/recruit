package com.samsung.ssm1_1;

import java.util.Scanner;

/*
 * 2016.02.02
 * by KMJ
 */

/*
 * 숫자 n을 입력받고 n(0<n<=100)의 약수의 개수를 구하시오
 */
public class Main {
	private static int T;
	private static int n[];
	
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		T = sc.nextInt();
		n = new int[T];
		
		for (int i = 1; i <= T; i++) {
			n[i-1] = sc.nextInt();
			System.out.println("Testcase" + i);
			System.out.println(getDivisorNum(sc.nextInt()));
		}

		sc.close();
	}

	public static int getDivisorNum(int n) {
		int count = 0;
		for (int i = 1; i <= n; i++) {
			if (n % i == 0) {
				count++;
			}
		}
		return count;
	}

}
