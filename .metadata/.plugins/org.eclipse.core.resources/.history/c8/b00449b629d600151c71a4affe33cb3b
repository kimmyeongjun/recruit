import java.util.Scanner;

public class BS {
	public int intArr[];
	public Scanner sc;

	public static void main(String[] args) {
		BS bs = new BS();
		bs.start();
	}

	public void start() {
		init();
		run();
	}

	public void init() {
		sc = new Scanner(System.in);
		System.out.println("사이즈 입력!");
		intArr = new int[sc.nextInt()];
	}

	public void run() {
		int find = 0, index;
		for (int i = 0; i < intArr.length; i++) {
			intArr[i] = sc.nextInt();
		}
		System.out.println("찾고자 하는 수 입력");
		find = sc.nextInt();
		index = BinarySearch(intArr, find);

		if (index != -1) {
			System.out.println("찾고자하는 수의 인덱스  = " + index);
		} else {
			System.out.println("그런 수 없다");
		}

	}

	public static int BinarySearch(int intArr[], int find) {
		int start = 0;
		int end = intArr.length - 1;
		int mid = (start + end) / 2;
		while(start<=end){
			if(intArr[mid]==find){
				return mid;
			}else if(intArr[mid]<find){
				start = mid+1;
			}else{
				end = mid -1;
			}
		}
		return -1;
	}

}
