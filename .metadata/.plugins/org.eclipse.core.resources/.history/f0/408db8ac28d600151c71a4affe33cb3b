import java.util.Scanner;

public class BS {
	public int intArr[];
	public Scanner sc;
	public static void main(String[] args){
		BS bs = new BS();
		bs.start();
	}
	public void start(){
		init();
		run();
	}
	public void init(){
		sc = new Scanner(System.in);
		System.out.println("사이즈 입력!");
		intArr = new int[sc.nextInt()];
		
	}

}
