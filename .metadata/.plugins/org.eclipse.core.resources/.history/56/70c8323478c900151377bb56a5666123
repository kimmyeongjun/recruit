package com.samsung.ssm1_2;

import java.util.Scanner;

public class Main {
	private static int T;
	private static int n[];

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		T = sc.nextInt();
		n = new int[T];

		for (int i = 0; i < T; i++) {
			n[i] = sc.nextInt();
		}

		sc.close();
	}

	public static String getHexa(int n) {
		String hexa = "";
		return hexa;
	}

}
